    "type": "module",
    "scripts": {
        "dev": "npm run routes:generate && vite",
        "routes:generate": "node node_modules/@tanstack/router-cli/bin/tsr.cjs generate --routes-dir=src/routes",
        "build": "npm run routes:generate && tsc && vite build",
        "preview": "vite preview",
