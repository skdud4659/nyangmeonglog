        "lucide-react": "^0.534.0",
        "pretendard": "^1.3.9",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.60.0",
        "vite-plugin-pwa": "^1.0.1",
        "zod": "^4.0.5",
        "zustand": "^5.0.6"
