{
    "version": 2,
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [{ "source": "/(.*)", "destination": "/" }],
    "headers": [
        {
            "source": "/sw.js",
            "headers": [
                { "key": "Cache-Control", "value": "no-cache" },
                { "key": "Service-Worker-Allowed", "value": "/" }
            ]
        }
    ]
}
